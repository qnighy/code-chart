{
  "name": "code-chart",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@4.10.3",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "yarn generate && yarn build:next",
    "build:next": "next build",
    "fmt": "prettier -w .",
    "fmt:check": "prettier -c .",
    "generate": "node --import jiti/register ./app/lib/ucd/generate.ts",
    "start": "next start",
    "test": "vitest",
    "lint": "eslint"
  },
  "dependencies": {
    "@qnighy/internal-public-fetch": "workspace:*",
    "lucide-react": "^0.545.0",
    "next": "^15.5.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "virtua": "^0.45.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/adm-zip": "^0.5.7",
    "@types/node": "^24.7.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "adm-zip": "^0.5.16",
    "eslint": "^9.37.0",
    "eslint-config-next": "^15.5.4",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
